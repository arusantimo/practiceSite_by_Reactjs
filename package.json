{
  "name": "react-test-story",
  "version": "1.0.3",
  "description": "Website for reactjs test",
  "main": "index.ts",
  "author": "Chang Gyu Lee <ziziana0507@gmail.com> (https://arusantimo.github.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamdew/react-express-boilerplate.git"
  },
  "keywords": [
    "react",
    "react server",
    "reactjs",
    "react express",
    "express",
    "boilerplate",
    "server",
    "serverside",
    "test"
  ],
  "scripts": {
    "=======================================__utils__=========================================": "",
    "clean": "rm -rf build public/bundle.js public/*.css",
    "=======================================__develop__=======================================": "",
    "start": "yarn run start:dev",
    "start:dev": "yarn install && yarn run clean && node ./scripts/development.js",
    "=======================================__deploy__========================================": "",
    "_build": "yarn install && yarn run clean && ./node_modules/.bin/babel server --out-dir build && ./node_modules/.bin/webpack --config ./configs/webpack.config.prod.js",
    "start:prod": "yarn run _build && pm2 start ./scripts/production.js --name 'basic' -i 2",
    "reload": "yarn run build && pm2 reload basic",
    "stop": "pm2 stop basic && pm2 delete basic",
    "=======================================__tests__=========================================": "",
    "test": "yarn build && ./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --recursive server/**/*.spec.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.0",
    "dotenv": "^4.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.0",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "asset-require-hook": "^1.0.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.2",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "mocha": "^3.1.2",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "path": "^0.12.7",
    "postcss-loader": "^1.3.3",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.3",
    "should": "^11.1.1",
    "supertest": "^2.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
